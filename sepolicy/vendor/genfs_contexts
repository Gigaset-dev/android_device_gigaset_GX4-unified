# Battery
genfscon sysfs /devices/platform/soc/11f00000.i2c/i2c-7/7-0066/power_supply                  u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/soc/11e00000.i2c/i2c-0/0-0071/power_supply                  u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/soc/11eb0000.i2c/i2c-2/2-006b/power_supply                  u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/virtual/hf_manager/hf_manager/sar_cali                               u:object_r:sysfs_batteryinfo:s0

# LEDs
genfscon sysfs /devices/platform/soc/11eb3000.i2c/i2c-9/9-0064/leds                          u:object_r:sysfs_leds:s0

# Wakeup
genfscon sysfs /devices/platform/1a000000.camisp_legacy/wakeup                               u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/11017000.i2c/i2c-5/5-004e/wakeup                        u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/charger/power_supply/cw-bat/wakeup14                        u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/11015000.i2c/i2c-3/3-0008/wakeup                        u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/10228000.gce/wakeup                                     u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/11e00000.i2c/i2c-0/0-0071/power_supply/bms/wakeup18     u:object_r:sysfs_wakeup:s0
